{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "strict": true,
    "baseUrl": ".",
    "paths": {},
    "types": ["node", "react", "react-dom"],
    "allowJs": false,
    "noEmit": true,
    "skipLibCheck": true
  },
  "include": ["src", "vite.config.ts"],
  "exclude": ["node_modules", "build"]
}
